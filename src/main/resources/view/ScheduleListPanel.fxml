     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Label text="Description" />
         <Label text="Date" GridPane.columnIndex="1" />
         <Label text="Time" GridPane.columnIndex="2" />
      </children>
   </GridPane>
